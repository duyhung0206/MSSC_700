/**
 * Affiliate's navigation template
 */
?>
<div class="div-navigantion-credit col-lg-3 col-md-3 col-sm-3 col-xs-12" style="padding: 0;margin-bottom: 15px;">
    <!--By Crystal: add class title-->
    <div class="title-credit">
        <h2 class="title-credit-label navigation-credit"><?php echo $this->getNavigationTitle(); ?></h2>
    </div>
    <!--end edit-->
    <div class="block-content">
        <ul id="customercredit-navigation-<?php echo $this->getNameInLayout() ?>">
            <?php $_links = $this->getLinks(); ?>
            <?php foreach ($_links as $_link): ?>
                <?php if ($_link->getDisabled()) continue; ?>
                <?php if ($this->isActive($_link)): ?>
                    <li class="current"><strong><?php echo $_link->getLabel() ?></strong></li>
                        <?php else: ?>
                    <li><a href="<?php echo $_link->getUrl() ?>"><?php echo $_link->getLabel() ?></a></li>
                <?php endif; ?>
            <?php endforeach; ?>
        </ul>
        <script type="text/javascript">
            decorateGeneric($('customercredit-navigation-<?php echo $this->getNameInLayout() ?>').childElements(), ['last']);
        </script>
    </div>
</div>