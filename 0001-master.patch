+                $memberPackage->setEndTime($endTime);
+                $memberPackage->setData('order_ids', $orderId);
+                $memberPackage->updatePackageStatus();
+                $memberPackage->save();
+            }
+
+            Mage::helper('membership/email')->sendEmailNewPackage($memberPackage);
+        } catch (Exception $e) {
+            echo($e->getMessage());
+        }
+
+        return;
+    }
