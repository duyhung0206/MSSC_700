+        $code = $credit_code->addCreditCode($friend_email, $credit_amount, Magestore_Customercredit_Model_Status::STATUS_UNUSED, $customerId);
+        $this->sendCodeToFriendEmail($friend_email, $credit_amount, $message, $code);
+    }
