+        $result = array();
+        $email = $this->getRequest()->getParam('email');
+        $websiteId = Mage::app()->getWebsite()->getId();
+        $existed = Mage::getModel('customer/customer')->getCollection()
+                ->addFieldToFilter('email', $email)
