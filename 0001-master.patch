+            var ev = document.createEvent('HTMLEvents');
+            ev.initEvent(evnt, true, true);
+            el.dispatchEvent(ev);
+        } else if (el['on' + evnt]) { // alternatively use the traditional event model (IE5)
+            el['on' + evnt]();
+        }
