+class Magestore_Customercredit_Helper_Data extends Mage_Core_Helper_Data
+{
+
+    public function isDisabled($store = null)
+    {
+        return Mage::getStoreConfig('advanced/modules_disable_output/Magestore_Customercredit', $store);
+    }
