+                $prefix = 'notice';
+                break;
+            case 2:
+                $prefix = 'critical';
+                break;
+            case 3:
+            default:
+                $prefix = 'major';
+        }
+        return '<span class="grid-severity-' . $prefix . '"><span>' . $statuses[(int) $row->getStatus()] . '</span></span>';
+    }
+
+}
